{
    "compilerOptions": {
      "module": "ES6",
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "forceConsistentCasingInFileNames": true,
      "sourceMap": true,
      "jsx": "react",
      "noImplicitAny": true,
      "strictNullChecks": true
    },
    "exclude": [
      "node_modules"
    ],
    "include": [
        "./src"
    ]
  }